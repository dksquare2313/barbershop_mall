      // function that prints the prices
    function printPrice(price,element){
      for(i in price){

          var row = document.createElement("div");
          row.classList.add('row');

          var col = document.createElement("div");
          col.classList.add('col-sm');

          var col2 = document.createElement("div");
          col2.classList.add('col');
          col2.classList.add('text-right');
          col2.classList.add('mt-2');

          row.appendChild(col);
          row.appendChild(col2);

          var title = document.createElement("h3");
          var desc = document.createElement("p");
          title.innerHTML = price[i].title;
          desc.innerHTML = price[i].desc;



          col.appendChild(title);
          col.appendChild(desc);
          col2.innerHTML = "$"+price[i].price;

          element.appendChild(row);

          var hr = document.createElement("hr");
          element.appendChild(hr);
      }
    }
  
  </script>

